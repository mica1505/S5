package up.mi.cm.td1;

public class NbPremiers {
	public static void main(String [] args) {
		System.out.println(allIntegers(13));
	}
	static boolean allIntegers(int n) {
		boolean premier = true;
		int i,k;
			System.out.println((i*2)+1);
			if(n%((i*2)+1)==0) {
				premier = false;
			}
		}
		return premier;
	}
}
