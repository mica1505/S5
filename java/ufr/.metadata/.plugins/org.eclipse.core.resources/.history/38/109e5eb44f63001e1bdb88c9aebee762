package up.mi.cm.td2;

public class Point {
	
	private int x,y;
	
	public Point(int x,int y) {
		this.x = x;
		this.y = y;
	}
	
	public int getX() {
		return this.x;
	}
	public int getY() {
		return this.y;
	}
	public double Distance(Point p) {
		double x = p.getX() - this.getX();
		double y = p.getY()  - this.getY();
		return Math.sqrt((x*x)+(y*y));
	}
}
